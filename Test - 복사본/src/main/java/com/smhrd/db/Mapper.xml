<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.db.Mapper">
	<!-- SQL 쿼리 정의 -->

	<insert id="UploadCal"
		parameterType="com.smhrd.model.CalendarDTO">
		INSERT INTO CAL_TEST VALUES(#{title}, #{start}, #{end})
	</insert>

	<select id="GetAllEvents"
		resultType="com.smhrd.model.CalendarDTO">
		SELECT * FROM CAL_TEST
	</select>
</mapper>
